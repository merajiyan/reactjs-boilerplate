{
  "name": "reactjs-boilerplate",
  "version": "2.0.3",
  "description": "",
  "main": "index.tsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nikhil-Kumaran/reactjs-boilerplate/"
  },
  "homepage": "https://reactjs-boilerplate.netlify.com/",
  "keywords": [
    "react-boilerplate",
    "reactjs-boilerplate",
    "create-react-app",
    "CRA",
    "reactjs-setup",
    "reactjs",
    "webpack",
    "babel"
  ],
  "author": "Sina Merajiyan",
  "license": "ISC",
  "engines": {
    "node": ">=10.16.0"
  },
  "bin": {
    "reactjs-boilerplate": "./bin/start.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      [
        "@babel/preset-typescript",
        {
          "isTSX": true,
          "allExtensions": true
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ]
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "pretty-quick --staged",
      "eslint",
      "git add"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --port 8080 --mode=development --display-error-details",
    "clean-build": "shx rm -rf build",
    "build": "yarn clean-build && webpack -p --mode=production",
    "build-serve": "yarn build && serve"
  },
  "dependencies": {
    "fs-extra": "^9.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@hot-loader/react-dom": "^17.0.1",
    "react-hot-loader": "^4.12.21",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@svgr/webpack": "^5.4.0",
    "@types/classnames": "^2.3.1",
    "@types/dotenv-webpack": "^7.0.2",
    "@types/figlet": "^1.5.1",
    "@types/node": "^15.14.1",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack": "^5.28.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-dev-server": "^3.11.5",
    "@types/webpack-merge": "^5.0.0",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "file-loader": "^6.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.0",
    "compression-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-loader": "^4.0.2",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.7",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "less": "^3.11.3",
    "less-loader": "^6.2.0",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "serve": "^11.3.2",
    "shx": "^0.3.2",
    "style-loader": "^1.2.1",
    "typescript": "^4.3.4",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
