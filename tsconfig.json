{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "alwaysStrict": false,
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"]
    },
    "esModuleInterop": true,
    "importsNotUsedAsValues": "preserve",
    "jsx": "preserve",
    "module": "CommonJS",
    "moduleResolution": "Node",
    "noEmit": true,
    "noImplicitAny": true,
    "target": "ES5",
    "strictNullChecks": true,
    "resolveJsonModule": true
  }
}
